<h1>Your local forecast!</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Date</th> <th>High</th> <th>Low</th> <th>Sunrise / Sunset</th>
    </tr>
  </thead>
  <tbody>
  {{#each sevenDayForecast}}
    <tr>
      <td>{{date}}</td> <td>{{maxTemp}}</td> <td>{{minTemp}}</td> <td>{{sunrise}} / {{sunset}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>